<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.micro-p.com/mdfs/fulfillment">
    <xs:element name="stockCheckResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="requestEcho" type="requestEchoType" />
